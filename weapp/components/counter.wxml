
<view class="counter {{style}}">
  <button size="mini" data-wpy-evt="19-5" bindtap="_proxy">  +  </button>
  <button size="mini" data-wpy-evt="19-6" bindtap="_proxy">  -  </button>
  <button size="mini" data-wpy-evt="19-7" bindtap="_proxy">  INCREMENT  </button>
  <button size="mini" data-wpy-evt="19-8" bindtap="_proxy">  DECREMENT  </button>
  <button size="mini" data-wpy-evt="19-9" bindtap="_proxy">  ASYNC INCREMENT  </button>
  <label class="count"> {{counter}} </label>
</view>
