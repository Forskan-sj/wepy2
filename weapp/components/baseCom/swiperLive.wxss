.formblock {
  opacity: 0.5;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
}
.formblock .button_nostyle {
  background: red;
  width: 100%;
  height: 100%;
}
.LiveWin {
  width: 702rpx;
  height: 300rpx;
  margin: 0 auto;
  background: #fef7f1;
  border-radius: 8rpx 8rpx 0 0;
  display: flex;
  overflow: hidden;
  box-sizing: border-box;
  border: 4rpx solid #e9b37e;
}
.LiveWin .LiveCover {
  width: 256rpx;
  height: 296rpx;
  position: relative;
}
.LiveWin .LiveCover .LiveStatus {
  font-size: 22rpx;
  color: #fff;
  position: absolute;
  top: 12rpx;
  left: 22rpx;
  width: 120rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: rgba(1, 1, 1, 0.4);
  border-radius: 8rpx;
}
.LiveWin .LiveCover .LiveStatus .status {
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
}
.LiveWin .LiveCover .image {
  width: 100%;
  height: 100%;
}
.LiveWin .LiveCover .LiveNum {
  /* width:142/@p;
      height:40/@p; */
  display: inline-block;
  padding: 4px 8rpx;
  height: 28rpx;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 8rpx 0px 0px 0px;
  /* opacity:0.4; */
  position: absolute;
  bottom: 0;
  color: #fff;
  font-size: 20rpx;
  z-index: 1;
  right: 0;
}
.LiveWin .LiveInfo {
  width: 445rpx;
  height: 100%;
  box-sizing: border-box;
  padding: 18rpx 10rpx 18rpx 18rpx;
}
.LiveWin .LiveInfo .LiveTitle {
  color: #111111;
  font-size: 34rpx;
  font-weight: 600;
  height: 48rpx;
  overflow: hidden;
  display: block;
  white-space: nowrap;
  text-overflow: ellipsis;
  line-height: 48rpx;
}
.LiveWin .LiveInfo .LiveExplain {
  font-size: 22rpx;
  color: #7b7b7b;
  height: 32rpx;
  line-height: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.LiveWin .LiveInfo .LiveList {
  display: flex;
  width: 100%;
  justify-content: space-around;
  margin-top: 32rpx;
}
.LiveWin .LiveInfo .LiveList .goods {
  width: 100rpx;
  height: 100rpx;
  border-radius: 8rpx;
  background: gray;
  overflow: hidden;
}
.LiveWin .LiveInfo .LiveList .goods .image {
  width: 100rpx;
  height: 100rpx;
}
.LiveWin .LiveInfo .LiveList .goodsNum {
  font-size: 20rpx;
  text-align: center;
  padding: 20rpx 10rpx 24rpx;
  background: #FFF3F3;
  color: #FE1969;
}

