.formblock {
  opacity: 0.2;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
}
.formblock .button_nostyle {
  background: red;
  width: 100%;
  height: 100%;
}
.s-nullpage {
  display: flex;
  width: 100%;
  height: 100%;
  overflow: hidden;
  text-align: center;
  color: #bfbfbf;
  flex-direction: column;
}
.s-nullpage .s-nullpage-middle {
  margin-top: 150rpx;
}
.s-nullpage .s-nullpage-middle .text {
  font-size: 250rpx;
  line-height: 20rpx;
}
.s-nullpage .s-null-tip {
  margin-top: -110rpx;
}
.s-nullpage .s-null-tip .text {
  display: block;
  font-size: 28rpx;
  color: #796A50;
  padding-top: 100rpx;
  width: 280rpx;
  margin: 0 auto;
  text-align: center;
}
.s-nowiff {
  width: 396rpx;
  height: 308rpx;
  background: url('https://img.jinrijianlou.com/jpc_picture/a/58e5788c6eb81.png') no-repeat;
  background-size: 100% 100%;
  display: inline-block;
}

