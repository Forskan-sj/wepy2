.container {
  background: #1E1A19;
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.contentBox {
  /* height: 380rpx; */
  padding-top: 40rpx;
  box-sizing: border-box;
  width: 100%;
  background: #1E1A19;
  overflow: hidden;
  position: relative;
}
.contentBox .GoodInfo {
  display: flex;
  flex-direction: column;
  height: 300rpx;
  overflow: hidden;
  width: 702rpx;
  position: relative;
  background: #ffffff;
  box-shadow: 0px 0px 10rpx 6rpx rgba(1, 1, 1, 0.1);
  border-radius: 12rpx 12rpx 0 0;
  /* padding: 32rpx 24rpx; */
  box-sizing: border-box;
  margin: 0 auto;
  margin-bottom: 40rpx;
  border: 4rpx solid #e9b37e;
}
.contentBox .GoodInfo .slide-image {
  width: 702rpx;
  height: 300rpx;
}
