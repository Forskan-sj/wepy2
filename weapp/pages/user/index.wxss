.formblock {
  opacity: 0.2;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
}
.formblock .button_nostyle {
  background: red;
  width: 100%;
  height: 100%;
}
page {
  height: 100%;
}
.container {
  position: absolute;
  width: 100%;
}
.container .imgbk {
  width: 150rpx;
  height: 150rpx;
  display: block;
  margin: 300rpx auto 0;
  border-radius: 150rpx;
}
.container .confirm {
  width: 690rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 200rpx;
  color: #CD9967;
  background: linear-gradient(270deg, #3d3634 0%, #1e1a19 100%);
}
.container .text {
  color: #aaa;
  font-size: 30rpx;
  margin-top: 25rpx;
  text-align: center;
  display: block;
}

